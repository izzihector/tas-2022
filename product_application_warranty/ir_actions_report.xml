<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data>

        <record id="paperformat_msa_report" model="report.paperformat">
            <field name="name">MSA REPORT</field>
            <field name="default" eval="True"/>
            <field name="format">Letter</field>
            <field name="orientation">Portrait</field>
            <field name="margin_top">10</field>
            <field name="margin_bottom">20</field>
            <field name="margin_left">18</field>
            <field name="margin_right">10</field>
            <field name="header_line" eval="False"/>
            <field name="header_spacing">55</field>
            <field name="dpi">80</field>
        </record>

        <record id="product_application_warranty_report" model="ir.actions.report">
            <field name="name">Warranty Report</field>
            <field name="report_name">product_application_warranty.warranty_report_template</field>
            <field name="report_type">qweb-pdf</field>
            <field name="model">project.project</field>
            <field name="report_file">product_application_warranty.warranty_report_template</field>
            <field name="print_report_name">object.name</field>
            <field name="paperformat_id" ref="product_application_warranty.paperformat_msa_report"/>
        </record>

            <!-- attachment="('GAP_%s_pdf'%object.name)" -->
    </data>
</odoo>

