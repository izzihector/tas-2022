<odoo>
    <record id="project_form" model="ir.ui.view">
        <field name="name">project.form</field>
        <field name="model">project.project</field>
        <field name="inherit_id" ref="project.edit_project"/>
        <field name="arch" type="xml">
            <field name="privacy_visibility" position="after">
                <field name="garantia_producto_id"/>
                <field name="cobertura_inicio"/>
                <field name="cobertura_fin"/>
                <field name="garantia_pediodo"/>
                <field name="direccion_proyecto"/>
                <field name="beneficiario"/>
            </field>
            
        </field>
    </record>

</odoo>
